<div class="content-wrapper">
<!-- Example DataTables Card-->
  <div clase="serch-conditions">
    <form action="/home/tables" method="get" style="margin: 10px;">
      <input type="submit" value="검색" class="btn btn-primary" style="float: right; margin: 10px;">
