              <th scope="col">
                <label for="gold" style="margin: 0rem 1rem">황금알점수</label>
                <select id="gold" name="gold" class="form-control" style="display: block; margin: 0rem 1rem">
                  <option value="1">아주높음</option>
                  <option value="2">높음</option>
                  <option value="3">보통</option>
                  <option value="4">낮음</option>
                  <option value="5">아주낮음</option>
                </select>
              </th>
              <th scope="col">
                <label for="power" style="margin: 0rem 1rem">파워등급</label>
                <select id="power" name="power" class="form-control" style="display: block; margin: 0rem 1rem">
                  <option value="10000">1등급</option>
                  <option value="11000">2등급</option>
                  <option value="11100">3등급</option>
                  <option value="11110">4등급</option>
                  <option value="11111">5등급</option>
                </select>
              </th>
              <th scope="col">
                <label for="supply" style="margin: 0rem 1rem">수급등급</label>
                <select id="supply" name="supply" class="form-control" style="display: block; margin: 0rem 1rem">
                  <option value="10000">1등급</option>
                  <option value="11000">2등급</option>
                  <option value="11100">3등급</option>
                  <option value="11110">4등급</option>
                  <option value="11111">5등급</option>
                </select>
              </th>
              <th scope="col">
                <label for="performance" style="margin: 0rem 1rem">실적등급</label>
                <select id="performance" name="performance" class="form-control" style="display: block; margin: 0rem 1rem">
                  <option value="10000">1등급</option>
                  <option value="11000">2등급</option>
                  <option value="11100">3등급</option>
                  <option value="11110">4등급</option>
                  <option value="11111">5등급</option>
                </select>
              </th>
              <th scope="col">
                <label for="value" style="margin: 0rem 1rem">현재가치</label>
                <select id="value" name="value" class="form-control" style="display: block; margin: 0rem 1rem">
                  <option value="1">저평가</option>
                  <option value="2">보통이하</option>
                  <option value="3">보통이상</option>
                  <option value="4">고평가</option>
                </select>
              </th>
            </tr>
